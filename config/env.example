# Binance API 配置
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET=your_binance_secret_here

# Binance Hedge API 配置（可选，仅在 HEDGE_ENABLED=true 时需要）
# 如果启用对冲功能，需要配置对冲账户的API凭证
HEDGE_ENABLED=false
BINANCE_API_KEY_HEDGE=your_binance_api_key_hedge_here
BINANCE_SECRET_HEDGE=your_binance_secret_hedge_here

# DeepSeek API 配置
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Discord 配置
DISCORD_WEBHOOK_URL=your_discord_webhook_url_here
DISCORD_ACCOUNT_TAG=your_account_tag_here
